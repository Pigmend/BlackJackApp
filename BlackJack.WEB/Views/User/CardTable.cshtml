@model IEnumerable<BlackJack.ViewModels.EntityViewModel.CardViewModel>

@{
    ViewBag.Title = "CardTable";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>CardTable</h2>
<body>
    <table>
        @foreach (var item in Model)
        {
            <tr>
                <td> @item.ID </td>
                <td> @item.CardName </td>
                <td> @item.CardScore </td>
            </tr>
        }
    </table>
</body>
